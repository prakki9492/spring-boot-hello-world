pipeline{
    agent 'any'

    stages {

        stage ("checkout stage") {

            steps{

                     echo "checking the code from git repository"
            }

           
        }

        stage("Builing stage"){

            steps{

            sh """
                ls -lrt
                mvn install
                cd target/
                ls -lrt
            """
            }
         
        }
    }
}